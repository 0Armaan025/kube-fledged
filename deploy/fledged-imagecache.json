{
  "apiVersion": "fledged.k8s.io/v1alpha1",
  "kind": "ImageCache", 
  "metadata": {
      "name": "imagecache1",
      "namespace": "kube-fledged"
    },
  "spec": {
    "cacheSpec": [
      {
        "images": [
          "senthilrch/my-private-app:1.0",
          "nginx"
        ]
      },
      {
        "images": [
          "redis",
          "jenkins",
          "docker"
        ],
        "nodeSelector": {
          "kubernetes.io/hostname": "worker1"
        }
      }
    ],
    "imagePullSecrets": [
      {
        "name": "myregistrykey"
      }
    ]
  }
}
